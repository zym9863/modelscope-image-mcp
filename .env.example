MODELSCOPE_SDK_TOKEN=your_api_key_here
 
 # 可选配置
 # MODELSCOPE_BASE_URL=https://api-inference.modelscope.cn/
 # MODELSCOPE_MODEL=Qwen/Qwen-Image
 
 # 日志级别（可选）：DEBUG/INFO/WARN/ERROR，默认 INFO
 MODELSCOPE_LOG_LEVEL=INFO
 
 # 轮询策略（可选）
 # 轮询基础间隔（秒），默认 5
 MODELSCOPE_POLL_INTERVAL_SECONDS=5
 # 最大轮询尝试次数，默认 120（约 10 分钟）
 MODELSCOPE_MAX_POLL_ATTEMPTS=120
 # 是否开启指数退避，默认 false
 MODELSCOPE_POLL_BACKOFF=false
 # 指数退避的最大间隔（秒），默认 30
 MODELSCOPE_MAX_POLL_INTERVAL_SECONDS=30